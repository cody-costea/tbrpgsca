<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tbrpgsca.library"
    android:versionCode="1" android:versionName="0.34">

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="19" />

    <application
        android:icon="@drawable/launcher"
        android:label="@string/app_name"
        android:allowBackup="true"
        android:theme="@android:style/Theme.Black" android:name="DataApp">
        <activity
            android:name="com.tbrpgsca.demo.DemoAct"
            android:label="@string/app_name" 
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="BattleAct"
            android:label="@string/app_name" 
            android:screenOrientation="landscape"
        	android:parentActivityName="com.tbrpgsca.demo.DemoAct">
        	<meta-data
            	android:name="android.support.PARENT_ACTIVITY"
            	android:value="com.tbrpgsca.demo.DemoAct" />
        </activity>
    </application>

</manifest>
