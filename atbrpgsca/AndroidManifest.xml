<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tbrpgsca.library"
    android:versionCode="57" android:versionName="0.557">

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="24" />

    <application
        android:icon="@drawable/launcher"
        android:label="@string/app_name"
        android:allowBackup="true"
        android:theme="@android:style/Theme.Black">
        <activity
            android:name="com.tbrpgsca.demo.DemoAct"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tbrpgsca.library.BattleAct"
            android:label="@string/app_name"
            android:screenOrientation="nosensor"
        	android:parentActivityName="com.tbrpgsca.demo.DemoAct">
        </activity>
    </application>

</manifest>
